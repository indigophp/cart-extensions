{
    "name": "indigophp/cart-extensions",
    "description": "Extensions for Indigo Cart",
    "license": "MIT",
    "keywords": ["shopping", "cart"],
    "homepage": "https://indigophp.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "indigophp/cart": "dev-develop"
    },
    "require-dev": {
        "mathiasverrases/money": "dev-nextrelease",
        "phpspec/phpspec": "~2.0",
        "henrikbjorn/phpspec-code-coverage" : "1.0.*@dev"
    },
    "suggest": {
        "sebastian/money": "Value Object that represents a monetary value using a currency's smallest unit."
    },
    "autoload": {
        "psr-4": {
            "Indigo\\Cart\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
